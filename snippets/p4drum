-- @p4drum
d'drum $ sound "[${1:k} ${2:sn} $1*2 [~ $2] [~ $1]]/5"
d'drum $ sound "[[$1 $1:1]*2 $2 $1*2 [~ $2] [~ $1]]/5"

d'drum $ sound "[$1*2 $2 [~ $1] $2]/4"
d'drum $ sound "[[$1 ~ ~ $1] $2 [~ $1] $2]/4"
