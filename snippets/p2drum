-- @p2drum
d'drum $ slow 7 $ s "${1:k} [${2:sn} $1] [$1 $2] [~ $2] [~ $1] $2 [$1 [$1 ${3:wtsn}]]"

d'drum $ 0.5 <~ sound "[[$1 $1:1] [$2 ~ ~ $1]]/2"

d'drum $ sound "[[$1 $2] [~ $1:1 $1:1 ~]]/2"
d'drum $ sound "[$1 [$2 $1:1 $1:1 ~]]/2"
