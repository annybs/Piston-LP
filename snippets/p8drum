-- @p8drum
d'drum $ within (0, 0.5) (superimpose ((1/3) ~>)) $ s "[${1:k} [${2:sn} ~ $1] [~ ~ $2] ~]/8"

d'drum $ s "[$1*2 [$2 ~ ~ $3] [~ $1] $2 [~ $1]]/5"
d'drum $ s "[$1 [$2 ~ ~ $3] [~ $1] $2]/4"

d'drum $ s "[$1 [$2 ~ ~ $1] [~ $1] $2]/4"

d'perc $ s "{${3:wt}/2, [~ ${4:wtsn}]/8}"

d'drum $ s "[$1 [$2 ~ ~ $1] [~ $1*2] $2]/4"
d'drum $ s "[$1*2 [$2 ~ ~ $1] [$1 $1*2] $2]/4"
